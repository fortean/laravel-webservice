{
    "name": "fortean/laravel-webservice",
    "type": "library",
    "description": "An Httpful-based web service provider for Laravel",
    "keywords": ["laravel", "webservice", "httpful", "rest"],
    "homepage": "https://github.com/fortean/laravel-webservice",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruce Walter",
            "email": "walter@fortean.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "^5.1",
        "nategood/httpful": "*"
    },
    "autoload": {
        "psr-4": {
            "Fortean\\Webservice\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Fortean\\Webservice\\Tests\\": "tests/"
        }
    }
}
